<ns2:Definitions xmlns:ns2="http://docs.oasis-open.org/tosca/ns/2011/12"
	xmlns:salsa="https://github.com/tuwiendsg/SALSA" id="M2MDaaS" name="M2MDaaS"
	salsa:defaultMetricsEnabled="true">
	<ns2:ServiceTemplate id="M2MDaaS">
		<ns2:BoundaryDefinitions />
		<ns2:TopologyTemplate>
			<ns2:RelationshipTemplate id="d" type="HOSTON">
				<ns2:SourceElement ref="QueueServiceUnit" />
				<ns2:TargetElement ref="mqtt_brokerVM" />
			</ns2:RelationshipTemplate>

			<ns2:RelationshipTemplate id="b" type="CONNECTTO">
				<ns2:SourceElement ref="brokerIp_Capability" />
				<ns2:TargetElement ref="brokerIp_Requirement" />
			</ns2:RelationshipTemplate>

			<ns2:RelationshipTemplate id="e" type="HOSTON">
				<ns2:SourceElement ref="LocalProcessingServiceUnit" />
				<ns2:TargetElement ref="processingVM" />
			</ns2:RelationshipTemplate>

			<ns2:RelationshipTemplate id="mqtt_broker"
				type="CONNECTTO">
				<ns2:SourceElement ref="brokerIp_Capability" />
				<ns2:TargetElement ref="brokerIp_Requirement_2" />
			</ns2:RelationshipTemplate>

			<ns2:RelationshipTemplate id="f" type="HOSTON">
				<ns2:SourceElement ref="sensor" />
				<ns2:TargetElement ref="sensorVM" />
			</ns2:RelationshipTemplate>

			<ns2:NodeTemplate minInstances="1" maxInstances="1"
				id="QueueServiceUnit" type="software">
				<ns2:Properties />
				<ns2:Requirements />
				<ns2:Capabilities>
					<ns2:Capability id="brokerIp_Capability" type="variable" />
				</ns2:Capabilities>
				<ns2:Policies />
				<ns2:DeploymentArtifacts>
					<ns2:DeploymentArtifact name="deployArtifactMQTT"
						artifactType="salsa:sh" artifactRef="deployArtifactMQTT" />
				</ns2:DeploymentArtifacts>
			</ns2:NodeTemplate>

			<ns2:NodeTemplate minInstances="1" maxInstances="2147483647"
				id="mqtt_brokerVM" type="os">
				<ns2:Properties>
					<MappingProperties>
						<MappingProperty type="os">
							<property name="provider">dsg@openstack</property>
							<property name="instanceType">000001920</property>
							<property name="baseImage">17ffd200-315f-4ba8-9e77-c294efc772bd
							</property>
							<property name="packages">gmetad,ganglia-monitor,openjdk-7-jre
							</property>
						</MappingProperty>
					</MappingProperties>
				</ns2:Properties>
				<ns2:Requirements />
				<ns2:Capabilities />
				<ns2:Policies />
			</ns2:NodeTemplate>
			<ns2:NodeTemplate minInstances="1" maxInstances="1"
				id="LocalProcessingServiceUnit" type="software">
				<ns2:Properties />
				<ns2:Requirements>
					<ns2:Requirement fulfilled="false" id="brokerIp_Requirement"
						type="variable" />
				</ns2:Requirements>
				<ns2:Capabilities />
				<ns2:Policies />
				<ns2:DeploymentArtifacts>
					<ns2:DeploymentArtifact name="deployArtifactProcessing"
						artifactType="salsa:sh" artifactRef="deployArtifactProcessing" />
				</ns2:DeploymentArtifacts>
			</ns2:NodeTemplate>
			<ns2:NodeTemplate minInstances="1" maxInstances="2147483647"
				id="processingVM" type="os">
				<ns2:Properties>
					<MappingProperties>
						<MappingProperty type="os">
							<property name="provider">dsg@openstack</property>
							<property name="instanceType">000001920</property>
							<property name="baseImage">9e6ee7a0-7dc9-4fe1-851a-7dc632f5f912
							</property>
							<property name="packages">gmetad,ganglia-monitor,openjdk-7-jre
							</property>
						</MappingProperty>
					</MappingProperties>
				</ns2:Properties>
				<ns2:Requirements />
				<ns2:Capabilities />
				<ns2:Policies />
			</ns2:NodeTemplate>
			<ns2:NodeTemplate minInstances="1" maxInstances="5"
				id="sensor" type="software">
				<ns2:Properties />
				<ns2:Requirements>
					<ns2:Requirement fulfilled="false" id="brokerIp_Requirement_2"
						type="variable" />
				</ns2:Requirements>
				<ns2:Capabilities />
				<ns2:Policies />
				<ns2:DeploymentArtifacts>
					<ns2:DeploymentArtifact name="deployArtifactSensor"
						artifactType="salsa:sh" artifactRef="deployArtifactSensor" />
				</ns2:DeploymentArtifacts>
			</ns2:NodeTemplate>
			<ns2:NodeTemplate minInstances="1" maxInstances="2147483647"
				id="sensorVM" type="os">
				<ns2:Properties>
					<MappingProperties>
						<MappingProperty type="os">
							<property name="provider">dsg@openstack</property>
							<property name="instanceType">000001920</property>
							<property name="baseImage">17ffd200-315f-4ba8-9e77-c294efc772bd
							</property>
							<property name="packages">openjdk-7-jre</property>
						</MappingProperty>
					</MappingProperties>
				</ns2:Properties>
				<ns2:Requirements />
				<ns2:Capabilities />
				<ns2:Policies />
			</ns2:NodeTemplate>
		</ns2:TopologyTemplate>
	</ns2:ServiceTemplate>
	<ns2:ArtifactTemplate id="deployArtifactMQTT"		type="sh">
		<ns2:Properties />
		<ns2:ArtifactReferences>
			<ns2:ArtifactReference
				reference="http://128.130.172.215/salsa/upload/files/DaasService/IoT/run_mqtt_broker.sh" />
		</ns2:ArtifactReferences>
	</ns2:ArtifactTemplate>
	<ns2:ArtifactTemplate id="deployArtifactProcessing"		type="sh">
		<ns2:Properties />
		<ns2:ArtifactReferences>
			<ns2:ArtifactReference
				reference="http://128.130.172.215/salsa/upload/files/DaasService/IoT/install-local-analysis-service.sh" />
		</ns2:ArtifactReferences>
	</ns2:ArtifactTemplate>
	<ns2:ArtifactTemplate id="deployArtifactSensor"		type="sh">
		<ns2:Properties />
		<ns2:ArtifactReferences>
			<ns2:ArtifactReference
				reference="http://128.130.172.215/salsa/upload/files/DaasService/IoT/run_sensor.sh" />
		</ns2:ArtifactReferences>
	</ns2:ArtifactTemplate>
</ns2:Definitions>